{
  "name": "dcarbon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env/.env.development next dev",
    "start:dev": "env-cmd -f .env/.env.development next dev",
    "build:dev": "env-cmd -f .env/.env.development next build",
    "start:stag": "env-cmd -f .env/.env.staging next start",
    "build:stag": "env-cmd -f .env/.env.staging next build",
    "start:main": "env-cmd -f .env/.env.production next start",
    "build:main": "env-cmd -f .env/.env.production next build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "pre-commit": "lint-staged",
    "postinstall": "husky",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.3",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@reduxjs/toolkit": "^1.9.5",
    "@solana/spl-token": "^0.4.1",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/wallet-adapter-wallets": "^0.19.31",
    "@solana/web3.js": "^1.91.1",
    "chart.js": "^4.4.2",
    "framer-motion": "^11.0.24",
    "next": "14.1.4",
    "pino-pretty": "^11.0.0",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.5",
    "swiper": "^11.1.0",
    "swr": "^2.2.5",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@hookform/resolvers": "^3.3.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.0.1",
    "axios": "^1.6.8",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "react-hook-form": "^7.51.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "yup": "^1.4.0"
  },
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  }
}
